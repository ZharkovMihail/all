#include <iostream> 
#include <stdio.h> 
using namespace std;
struct node {
	int data;
	node* left;
	node* right;
};
struct stract {
	node* head = NULL;
	node* pushall(int *a, int left, int right) {
		while (left <= right) {
			node* temp = new node;
			temp->data = a[(right - left) / 2];
			if (left == right) {
				return temp;
			}
			temp->left = pushall(a, left, ((right - left) / 2) - 1);
			temp->right = pushall(a, ((right - left) / 2) + 1, right);
			return temp;
		}
	}
	void show(node *head) {
		if (head != NULL) {
			show(head->left);
			cout << head->data;
			show(head->right);
		}
	}
	void delit(node *Tree) {
		if (Tree != NULL) {
			delit(Tree->left);
			delit(Tree->right);
			delete Tree;
		}
	}
};

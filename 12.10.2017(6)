#include <iostream>
using namespace std;
int main() {	
	for (int r = 0; r < 10; r++) {
		for (int c = 0; c < 10; c++) {
			for (int b = 0; b < 10; b++) {
				for (int g = 0; g < 10 - b; g++) {
					for (int a = 0; a < 10; a++) {
						cout << "d,e,f=0" << " a=" << a << " b=" << b << " c=" << c << " r=k=" << r << " g=" << g << " m=" << b + g << endl;
					}
				}
			}
		}
	}
	return 0;
}
